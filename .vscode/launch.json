{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

    
    
        {
            "name": "rgust-dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "deviceId": "chrome",
            "args": [
                "--web-renderer html",
                "--web-browser-flag",
                "--disable-web-security"
            ]
        },
        {
            "name": "rgust-prod",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_prod.dart",
            "deviceId": "chrome",
            "args": [
                "--web-browser-flag",
                "--disable-web-security"
            ]
        },
        {
            "name": "rgust-mobile",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
        },
        {
            "name": "rgust (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "rgust (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}